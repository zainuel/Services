<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zainuel.services">

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <activity android:name=".Progressbar" />
        <activity
            android:name="com.crazyhitty.chdev.ks.firebasechat.ui.activities.UserListingActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.crazyhitty.chdev.ks.firebasechat.ui.activities.ChatActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Tabbed0"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Admin.AdminHomeActivity"
            android:label="@string/title_activity_admin_home" />
        <activity android:name=".Admin.UploadDocsActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".ProjectDetailsSelectActivity" />
        <activity android:name=".TypeSelectionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuestionsActivity" />
        <activity android:name=".Admin.ProjectRequirementsActivity" />
        <activity android:name=".FeedbackActivity"></activity>
    </application>

</manifest>